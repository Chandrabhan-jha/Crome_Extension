{
    "manifest_version": 3,
    "name": "RMS-Extension",
    "description": "RMS-Extension for Add Candidate",
    "version": "1.0",
    "action": {
      "default_popup": "rmsLogin.html",
      "default_icon": "Logo.png"
    },
    
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "service-worker.js",
      "type": "module"
    },
    "side_panel": {
    "default_path": "rmsLogin.html"
  },
  
  "permissions": ["sidePanel", "contextMenus", "storage"],
  "icons": {
    "16": "Logo.png",
    "48": "Logo.png",
    "128": "Logo.png"
  }
  }